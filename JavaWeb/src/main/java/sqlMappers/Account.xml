<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lxl.web.dao.AccountDao">

    <select id="getAccountByPhone" resultType="com.lxl.web.model.Account">
        SELECT
        *
        FROM account a
        WHERE a.mobilePhone=#{phone}
    </select>
</mapper>